{
  "name": "root",
  "author": "SunLxy <1011771396@qq.com>",
  "private": true,
  "description": "使用 rspack 进行封装.",
  "repository": {
    "type": "git",
    "url": "https://github.com/SunLxy/saqu-vue.git"
  },
  "scripts": {
    "prepare": "husky install",
    "----saqu-vue👇🏻----": "--------start-------",
    "build:core": "npm run-script build --workspace @saqu-vue/core",
    "watch:core": "npm run-script watch --workspace @saqu-vue/core",
    "version": "lerna version --force-publish=* --no-changelog --no-git-tag-version --no-push",
    "publish": "lerna publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "remove": "lerna exec \"rm -rf node_modules build  package-lock.json\"",
    "clean": "lerna clean --yes && npm run remove"
  },
  "dependencies": {
    "husky": "~8.0.0",
    "lerna": "~5.4.0",
    "prettier": "~2.7.0",
    "pretty-quick": "~3.1.3",
    "lint-staged": "~13.0.0",
    "tsbb": "~4.1.3"
  },
  "lint-staged": {
    "*.{js,jsx,tsx,ts,less,md,json}": "prettier --write"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "workspaces": [
    "core"
  ]
}